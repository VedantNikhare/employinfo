import javax.swing.*;
import java.awt.*;
class passwordf extends JFrame{
    JLabel l1;
    JPasswordField k1;
    passwordf(){
        setSize(1600, 1600);
        setVisible(true);
        l1=new JLabel("Enter the password");
        k1=new JPasswordField(20);
        setLayout(new FlowLayout());
        add(l1);
        add(k1);

    }
}

public class passwordfields {
    public static void main(String[] args) {
        new passwordf();
    }
    
}
